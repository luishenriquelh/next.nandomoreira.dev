  "scripts": {
    "clean": "rm -rf coverage .next out",
    "dev": "next dev",
    "build": "yarn clean && next build",
    "export": "next export",
    "start": "next start",
    "deploy": "yarn build && next export && netlify deploy --prod",
    "lint": "yarn eslint",
    "eslint": "eslint . --ext \".js,.ts,.tsx,.jsx\" --max-warnings=0",
    "prettier": "prettier --write .",
    "storybook": "start-storybook -s ./public -p 6006",
    "build-storybook": "build-storybook -s ./public",
    "test": "jest",
    "test:watch": "jest --watch --updateSnapshot --verbose",
    "test:coverage": "yarn clean && jest --collect-coverage"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*": [
      "yarn test --findRelatedTests --bail"
    ]
  },
